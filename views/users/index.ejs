<div class="container">
    <h1><%= title %></h1>
    
    <div class="users-grid">
        <% users.forEach(user => { %>
            <div class="user-card">
                <h3><%= user.name %></h3>
                <p>邮箱: <%= user.email %></p>
                <a href="/users/<%= user.id %>" class="btn btn-sm">查看详情</a>
            </div>
        <% }); %>
    </div>
    
    <a href="/" class="btn btn-primary">返回首页</a>
</div>