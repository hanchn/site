<div class="container">
    <h1><%= title %></h1>
    
    <div class="user-detail">
        <div class="user-info">
            <h2><%= user.name %></h2>
            <p><strong>ID:</strong> <%= user.id %></p>
            <p><strong>邮箱:</strong> <%= user.email %></p>
        </div>
    </div>
    
    <div class="actions">
        <a href="/users" class="btn btn-secondary">返回用户列表</a>
        <a href="/" class="btn btn-primary">返回首页</a>
    </div>
</div>